<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <!--
        Resource: asset.category
        -->
        <record id="asset_default_category" model="asset.category">
            <field name="name">No category</field>
            <field name="code">nocat</field>
            <field name="has_date_option" eval="False" />
        </record>
        
        <record id="seq_type_asset_asset" model="ir.sequence.type">
            <field name="name">Inventory Code</field>
            <field name="code">asset.asset</field>
        </record>
        <record id="seq_master_item" model="ir.sequence">
            <field name="name">Inventory Code</field>
            <field name="code">asset.asset</field>
            <field name="prefix">INV</field>
            <field name="padding">3</field>
        </record>
        
        <record id="seq_type_stock_production_lot" model="ir.sequence.type">
            <field name="name">Fake Serial Number</field>
            <field name="code">stock.production.lot</field>
        </record>
        <record id="seq_fake_serial" model="ir.sequence">
            <field name="name">Fake Serial Number</field>
            <field name="code">stock.production.lot</field>
            <field name="prefix">FAKE</field>
            <field name="padding">6</field>
        </record>
        
    </data>
</openerp>