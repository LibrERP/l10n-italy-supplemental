<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record model="ir.ui.view" id="midea.partner">
            <field name="name">Midea partner</field>
            <field name="model">midea.partner</field>
            <field name="type">form</field>
            <field name="priority" eval="16"/>
            <field name="arch" type="xml">
            <!-- view content: <form>, <tree>, <graph>, â€¦ -->
                <form string="My Idea">
                    <field name="name"/>
                    <field name="active"/>
                    <field name="state"/>
                </form>
            </field>
        </record>

        <record id="midea.action_test" model="ir.actions.act_window">
            <field name="name">My idea test record</field>
            <field name="res_model">midea.partner</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="midea.partner"/>
        </record>

        <menuitem id="menu_midea_partner" name="My idea"
                  parent="base.menu_administration" sequence="750" />
        <menuitem id="menu_midea_partner_test" name="My idea test"
                  action="midea.action_test"
                  parent="menu_midea_partner" sequence="50"/>

    </data>
</openerp>
